{
  "name": "react-gallery",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-lazyload": "^2.3.0",
    "react-scripts": "1.0.17",
    "react-transition-group": "1.x",
    "sanitize.css": "^5.0.0",
    "styled-components": "^2.2.3",
    "uuid": "^3.1.0"
  },
  "scripts": {
    "start":
      "NODE_PATH=src:src/components:src/containers:src/styled react-scripts start",
    "build":
      "NODE_PATH=src:src/components:src/containers:src/styled react-scripts build",
    "test":
      "NODE_PATH=src:src/components:src/containers:src/tests:src/styled react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  },
  "devDependencies": {
    "enzyme-to-json": "^3.2.2",
    "jest-styled-components": "^4.9.0"
  }
}
